<%- include('../partials/header') %>

<form id="new-form" action="/flights" method="POST">
  <label>Airline:</label>
  <select name="airline">
    <option value="American">American</option>
    <option value="Southwest">Southwest</option>
    <option value="united">United</option>
  </select>
  <label>Airport:</label>
  <select name="airport">
    <option value="AUS">AUS</option>
    <option value="DFK">DFK</option>
    <option value="DEN">DEN</option>
    <option value="LAX">LAX</option>
    <option value="SAN">SAN</option>
  </select>
  <label>FlightNo:</label>
  <input type="text" name="flightNo" placeholder="10-9999">
  <label>Departs:</label>
  <input type="type" name="departs" placeholder="mm/dd/yyy">
  <input type="submit" value="Add Flights" >
</form>
<%- include('../partials/footer') %>
